    <id>CSCore</id>
    <version>1.1.0</version>
    <title>CSCore</title>
    <authors>Florian R.</authors>
    <owners>Florian R.</owners>
    <licenseUrl>https://github.com/filoe/cscore/blob/master/license.md</licenseUrl>
